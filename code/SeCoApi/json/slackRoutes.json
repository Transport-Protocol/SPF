{
  "service_name": "SLACK",
  "grpc_ip": "localhost",
  "grpc_port": 50048,
  "grpc_service_name": "SlackMessaging",
  "grpc_package_name": "slackMessaging",
  "authentication_type": "OAUTH2",
  "requests": [
    {
      "http-method": "get",
      "route": "/slack/channelList",
      "grpc_function": "getChannelList",
      "response_parameter": [
        "channels"
      ]
    },
    {
      "http-method": "get",
      "route": "/slack/channel/messages",
      "query_parameter": [
        "channelId",
        "oldest"
      ],
      "grpc_function": "getChannelMessages",
      "response_parameter": [
        "messages",
        "timeStampOfLastMsg"
      ]
    },
    {
      "http-method": "post",
      "route": "/slack/sendMessage",
      "query_parameter": [
        "channelId",
        "message",
        "as_user"
      ],
      "grpc_function": "sendMessage",
      "response_parameter": [
        "status"
      ]
    }
  ]
}