{
  "service_name": "Owncloud",
  "grpc_ip": "localhost",
  "grpc_port": 50051,
  "requests": [
    {
      "http-method": "get",
      "route": "/owncloud/file",
      "query_parameter": [
        "path"
      ],
      "header_parameter": [
        "username",
        "password"
      ],
      "grpc_function": "getFile",
      "grpc_function_paramater": [
        "path",
        "username",
        "password"
      ],
      "response_parameter": [
        "fileName",
        "fileBuffer"
      ]
    },
    {
      "http-method": "get",
      "route": "/owncloud/filetree",
      "query_parameter": [
        "path"
      ],
      "header_parameter": [
        "username",
        "password"
      ],
      "grpc_function": "getFileTree",
      "grpc_function_paramater": [
        "path",
        "username",
        "password"
      ],
      "response_parameter": [
        "dirs"
      ]
    },
    {
      "http-method": "post",
      "route": "/owncloud/upload",
      "query_parameter": [
        "path"
      ],
      "header_parameter": [
        "username",
        "password"
      ],
      "grpc_function": "uploadFile",
      "grpc_function_paramater": [
        "path",
        "username",
        "password",
        "fileBuffer",
        "fileName"
      ],
      "response_parameter": [
        "status"
      ]
    }
  ]
}