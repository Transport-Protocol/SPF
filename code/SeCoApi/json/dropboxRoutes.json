{
  "service_name": "DROPBOX",
  "grpc_ip": "localhost",
  "grpc_port": 50052,
  "grpc_service_name": "FileStorage",
  "grpc_package_name": "fileStorage",
  "authentication_type": "OAUTH2",
  "requests": [
    {
      "http-method": "get",
      "route": "/dropbox/file",
      "query_parameter": [
        "path"
      ],
      "header_parameter": [
        "authorization"
      ],
      "grpc_function": "getFile",
      "response_parameter": [
        "fileName",
        "fileBuffer"
      ]
    },
    {
      "http-method": "get",
      "route": "/dropbox/filetree",
      "query_parameter": [
        "path"
      ],
      "header_parameter": [
        "authorization"
      ],
      "grpc_function": "getFileTree",
      "response_parameter": [
        "dirs"
      ]
    },
    {
      "http-method": "put",
      "route": "/dropbox/upload",
      "query_parameter": [
        "path"
      ],
      "header_parameter": [
        "authorization"
      ],
      "grpc_function": "uploadFile",
      "reserved_parameter": [
        "fileName",
        "fileBuffer"
      ],
      "response_parameter": [
        "status"
      ]
    }
  ]
}