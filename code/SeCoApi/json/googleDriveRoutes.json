{
  "service_name": "GOOGLE",
  "grpc_ip": "localhost",
  "grpc_port": 50049,
  "grpc_service_name": "FileStorage",
  "grpc_package_name": "fileStorage",
  "authentication_type": "OAUTH2",
  "requests": [
    {
      "http-method": "get",
      "route": "/google/file",
      "query_parameter": [
        "path"
      ],
      "grpc_function": "getFile",
      "response_parameter": [
        "fileName",
        "fileBuffer"
      ],
      "notification": false
    },
    {
      "http-method": "get",
      "route": "/google/filetree",
      "query_parameter": [
        "path"
      ],
      "grpc_function": "getFileTree",
      "response_parameter": [
        "dirs"
      ],
      "notification": false
    },
    {
      "http-method": "put",
      "route": "/google/file",
      "query_parameter": [
        "path"
      ],
      "grpc_function": "uploadFile",
      "reserved_parameter": [
        "fileName",
        "fileBuffer"
      ],
      "response_parameter": [
        "status"
      ],
      "notification": true,
      "message": "new file uploaded"
    },
    {
      "http-method": "post",
      "route": "/google/file/transfer",
      "query_parameter": [
        "path",
        "targetService"
      ],
      "grpc_function": "fileTransfer",
      "reserved_parameter": [
        "userName"
      ],
      "response_parameter": [
        "status"
      ],
      "notification": true,
      "message": "new file transfer"
    }
  ]
}