{
  "service_name": "BITBUCKET",
  "grpc_ip": "localhost",
  "grpc_port": 50071,
  "grpc_service_name": "VersionControl",
  "grpc_package_name": "versionControl",
  "authentication_type": "OAUTH2",
  "requests": [
    {
      "http-method": "get",
      "route": "/bitbucket/repositories",
      "grpc_function": "getRepositories",
      "response_parameter": [
        "repos"
      ]
    },
    {
      "http-method": "get",
      "route": "/bitbucket/repository/content",
      "query_parameter": [
        "path",
        "repositoryName"
      ],
      "grpc_function": "getRepositoryContent",
      "response_parameter": [
        "dirs"
      ]
    },
    {
      "http-method": "post",
      "route": "/bitbucket/repository/share",
      "query_parameter": [
        "repositoryName",
        "usernameToAdd"
      ],
      "grpc_function": "addUserToRepository",
      "response_parameter": [
        "status"
      ]
    }
  ]
}