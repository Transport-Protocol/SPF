create table umfrage (
    id int,          
    studiengang varchar(10),
    fachsemester int,
    webdienst varchar(30),
    nutzung int,
    zufrieden int,
    antrieb int,
    geraet int,
    applikation int,
    umfeld int,
    zugriffsrechte int,
    primary key (id, webdienst)
    );



